// Arithmetic operations test
print "=== Arithmetic Test ===";

// Basic arithmetic
var a = 10;
var b = 3;

print "Addition:";
print a;
print "+";
print b;
print "=";
print a + b;

print "Subtraction:";
print a;
print "-";
print b;
print "=";
print a - b;

print "Multiplication:";
print a;
print "*";
print b;
print "=";
print a * b;

print "Division:";
print a;
print "/";
print b;
print "=";
print a / b;

// Order of operations
print "Order of operations: 2 + 3 * 4";
print 2 + 3 * 4;

print "With parentheses: (2 + 3) * 4";
print (2 + 3) * 4;

// Negative numbers
var negative = -5;
print "Negative number:";
print negative;

print "Double negative:";
print -negative;

print "=== Arithmetic Test Complete ===";