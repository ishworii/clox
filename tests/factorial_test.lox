// Factorial calculation test
print "=== Factorial Test ===";

// Calculate factorial of 5
print "Calculating 5!:";
var num = 5;
var factorial = 1;
var original_num = num;

while (num > 1) {
    factorial = factorial * num;
    num = num - 1;
}

print original_num;
print "! =";
print factorial;

// Calculate factorial of 6 using for loop
print "Calculating 6! using for loop:";
var fact6 = 1;
for (var i = 1; i <= 6; i = i + 1) {
    fact6 = fact6 * i;
}
print "6! =";
print fact6;

// Calculate factorial of 0 and 1 (edge cases)
print "Edge cases:";
print "0! = 1";
print "1! = 1";

print "=== Factorial Test Complete ===";